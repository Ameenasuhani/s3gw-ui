{
  "name": "s3gw-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --verbose",
    "test": "jest --no-cache --watch",
    "test:ci": "jest --no-cache",
    "lint:eslint": "ng lint",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "run-p -csl --aggregate-output lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:scss": "stylelint \"**/*.scss\" --fix",
    "fix": "run-p -csl --aggregate-output fix:*",
    "i18n": "npm run i18n:extract && npm run i18n:pull && npm run i18n:convert",
    "i18n:extract": "transloco-keys-manager extract --sort --replace",
    "upgrade": "ncu -u --target minor"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/localize": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@fontsource/lato": "^4.5.10",
    "@fontsource/roboto-mono": "^4.5.10",
    "@mdi/font": "^7.2.96",
    "@ng-bootstrap/ng-bootstrap": "^13.1.1",
    "@ngneat/transloco": "^4.2.6",
    "aws-sdk": "^2.1377.0",
    "bootstrap": "5.2.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "ngx-toastr": "^15.2.2",
    "rxjs": "^7.4.0",
    "tslib": "^2.5.0",
    "validator": "^13.9.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^14.1.0",
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "^14.4.0",
    "@angular-eslint/eslint-plugin-template": "^14.4.0",
    "@angular-eslint/schematics": "^14.4.0",
    "@angular-eslint/template-parser": "^14.4.0",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@ngneat/transloco-keys-manager": "^3.7.0",
    "@types/crypto-js": "^4.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.194",
    "@types/node": "^16.18.30",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.9.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-linter": "^1.1.1",
    "htmllint-cli": "^0.0.7",
    "jest": "^28.1.3",
    "jest-canvas-mock": "^2.5.0",
    "jest-preset-angular": "^12.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "4.8.4"
  }
}
